{
  "name": "timesheets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "start": "npm run set-path && vite build && vite preview",
    "set-path": "echo $PATH && export PATH=$(npm bin):$PATH",
    "lint": "eslint 'src/**/*.{ts,tsx}' --ext .ts,.tsx --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --ext .ts,.tsx --fix --cache",
    "prebuild": "ls -la ./index.html && ls -la ./src"
  },
  "dependencies": {
    "@azure/msal-browser": "2.38.4",
    "@azure/msal-react": "1.5.13",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "axios": "^1.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.30.3",
    "vite": "^7.2.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prisma": "^7.3.0",
    "typescript": "^5.9.3"
  }
}
