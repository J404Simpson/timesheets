{
  "name": "timesheets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "echo $PATH && ls -la ./node_modules/.bin && npm run set-path && vite build && vite preview",
    "set-path": "export PATH=$(npm bin):$PATH",
    "lint": "eslint 'src/**/*.{ts,tsx}' --ext .ts,.tsx --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --ext .ts,.tsx --fix --cache",
    "postinstall": "if [ \"$SCM_BUILD\" = \"1\" ] && [ -f node_modules.tar.gz ]; then tar -xzf node_modules.tar.gz -C /home/site/wwwroot; else echo 'Skipping node_modules extraction'; fi"
  },
  "dependencies": {
    "@azure/msal-browser": "2.38.4",
    "@azure/msal-react": "1.5.13",
    "axios": "^1.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.17.0",
    "vite": "^7.2.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@prisma/client": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prisma": "^7.0.1",
    "typescript": "^5.9.3"
  }
}
